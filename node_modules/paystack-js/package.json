{
  "name": "paystack-js",
  "version": "0.18.1",
  "description": "Paystack's core javascript library",
  "main": "lib/paystack.js",
  "module": "es/paystack.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaystackHQ/js.paystack.co.git"
  },
  "keywords": [
    "paystack",
    "paystack js",
    "payments",
    "online",
    "nigeria"
  ],
  "author": "Paystack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaystackHQ/js.paystack.co/issues"
  },
  "homepage": "https://github.com/PaystackHQ/js.paystack.co#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "card-validator": "^4.2.0",
    "form-urlencoded": "^2.0.9",
    "is-mobile": "^0.2.2",
    "jsencrypt": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-c": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "fast-async": "^6.3.7",
    "http-server": "^0.11.1",
    "jest": "^22.4.4",
    "nodent-runtime": "^3.2.1",
    "prettier-eslint": "^8.8.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-json": "^2.3.1",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf lib dist es",
    "dev": "cross-env paystackEnvironment=dev nodemon src/dev.js --exec babel-node --presets airbnb,stage-2",
    "dev:popup": "npm run build:staging && http-server ./dev -o",
    "staging": "cross-env paystackEnvironment=staging nodemon src/dev.js --exec babel-node --presets airbnb,stage-2",
    "build:cjs": "cross-env NODE_ENV=cjs rollup --config rollup.config.paystack.js -o lib/paystack.js",
    "build:es": "cross-env NODE_ENV=es rollup --config rollup.config.paystack.js -o es/paystack.js",
    "build:paystackjs": "rollup --config rollup.config.paystack.js -o dist/paystack.js",
    "build:popup": "rollup --config rollup.config.popup.js -o dist/popup.js",
    "build": "npm run build:cjs && npm run build:es && cross-env NODE_ENV=production npm run build:paystackjs && cross-env NODE_ENV=production npm run build:popup",
    "build:dev": "npm run build:cjs && npm run build:es && cross-env NODE_ENV=dev npm run build:paystackjs && cross-env NODE_ENV=dev npm run build:popup",
    "build:staging": "npm run build:cjs && npm run build:es && cross-env NODE_ENV=staging npm run build:paystackjs && cross-env NODE_ENV=staging npm run build:popup",
    "lint": "eslint src/** --ignore-pattern '/src/dev.js'",
    "prepare": "npm run lint && npm run test && npm run clean && npm run build",
    "test": "cross-env NODE_ENV=test jest"
  },
  "jest": {
    "testUrl": "http://localhost/"
  }
}
